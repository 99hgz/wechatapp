.window{
  color:#4995fa;
  display: flex;
  align-items: center;
  justify-content: center;
}
.main{
  position: absolute;
  right:10px;
  left:10px;
}
.table{
  position: relative;
  top:10px;
}
.th {
  justify-content: center;
  background: #1a5cc8;
  color: #fff;
  display: flex;
  height: 30px;
  align-items: center;
  font-weight: bold;
  border:1px none #fff;
  border-style:none solid none solid; 
}
.tr{
  display: flex;
}
.td {
  justify-content: center;
  display: flex;
  min-height: 2rem;
  align-items: center;
  border:1px none #fff;
  border-style:none solid none solid; 
}

.main button{
  position: relative;
  top:20px;
  color:blue;
}

.oddrow{
  background-color: #D7EBFF;
}

.ac{
  color:rgb(0, 128, 0);
}